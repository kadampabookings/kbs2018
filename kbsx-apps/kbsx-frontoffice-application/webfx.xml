<?xml version="1.0" encoding="UTF-8"?>
<project applicationId="org.kadampabookings.kbs.frontoffice" applicationLabel="Kadampa bookings">

    <exported-packages>
        <source-packages/>
    </exported-packages>

    <dependencies>
        <used-by-source-modules/>

        <!-- Allow configuration variables from environment in Java executables (OpenJFX or Gluon) -->
        <plugin-module executable-target="java">webfx-platform-substitution-var-env-plugin</plugin-module>
        <!-- Allow configuration variables from local storage in GWT executables -->
        <plugin-module executable-target="gwt">webfx-platform-substitution-var-localstorage-plugin</plugin-module>
        <plugin-module executable-target="gwt">webfx-platform-substitution-var-windowlocation-plugin</plugin-module>

        <plugin-module>modality-base-frontoffice-min-plugin</plugin-module>
<!--
        <plugin-module>modality-crm-client-activity-login-plugin</plugin-module>
-->

        <!-- Event activities -->
        <plugin-module>kbsx-event-frontoffice-activities-all</plugin-module>

        <plugin-module>kbsx-ecommerce-frontoffice-activities-all</plugin-module>

    </dependencies>


</project>